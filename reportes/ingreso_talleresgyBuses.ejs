                    <td colspan="2" style="border: hidden;"> Gata y Palanca</td>
                    <td colspan="2" style="border: hidden;"> <%= Gata_y_Palanca; %></td>
                    <td colspan="2" style="border: hidden;"> Llanta Emergencia</td>
                    <td colspan="2" style="border: hidden;"> <%= Llanta_de_Emergencia; %></td>
                </tr>

                <tr style="border: hidden;">
                    <td colspan="2" style="border: hidden;"> CD/DVD</td>
                    <td colspan="2" style="border: hidden;"> <%= CD; %></td>
                    <td colspan="2" style="border: hidden;"> Llave Ruedas</td>
